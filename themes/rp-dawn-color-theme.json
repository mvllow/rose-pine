  "tokenColors": [
    /* comment. */
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#57527980",
        "fontStyle": "italic"
      }
    },
    /* constant. */
    {
      "scope": ["constant"],
      "settings": {
        "foreground": "#286983"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.charcter.escape"
      ],
      "settings": {
        "foreground": "#e09694"
      }
    },
    /* entity. */
    {
      "scope": ["entity.name"],
      "settings": {
        "foreground": "#e09694"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag",
        "entity.name.namespace",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#58a5b2"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#769b94"
      }
    },
    /* invalid. */
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#bd6d83"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#e1d5bd"
      }
    },
    /* keyword. */
    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "#286983"
      }
    },
    /* markup. */
    /* meta. */
    {
      "scope": ["meta.tag", "meta.brace"],
      "settings": {
        "foreground": "#575279"
      }
    },
    {
      "scope": ["meta.import", "meta.export"],
      "settings": {
        "foreground": "#286983"
      }
    },
    /* punctuation. */
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#57527980"
      }
    },
    {
      "scope": ["punctuation.accessor"],
      "settings": {
        "foreground": "#286983"
      }
    },
    {
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#769b94"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#58a5b280"
      }
    },
    /* source. */
    /* storage. */
    {
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#286983"
      }
    },
    /* string. */
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#769b94"
      }
    },
    /* support. */
    {
      "scope": ["support"],
      "settings": {
        "foreground": "#58a5b2"
      }
    },
    {
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#769b94"
      }
    },
    {
      "scope": ["support.function"],
      "settings": {
        "foreground": "#bd6d83"
      }
    },
    /* text. */
    /* variable. */
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#e09694",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable.other",
        "variable.language",
        "variable.function",
        "variable.argument"
      ],
      "settings": {
        "foreground": "#575279"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#9a86b1"
      }
    }
  ]
}
