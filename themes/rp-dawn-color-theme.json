  "tokenColors": [
    /* comment. */
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#65618e",
        "fontStyle": "italic"
      }
    },
    /* constant. */
    {
      "scope": ["constant"],
      "settings": {
        "foreground": "#2c677e"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.charcter.escape"
      ],
      "settings": {
        "foreground": "#cb9391"
      }
    },
    /* entity. */
    {
      "scope": ["entity.name"],
      "settings": {
        "foreground": "#cb9391"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag",
        "entity.name.namespace",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#7eb7c0"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#f8ae48"
      }
    },
    /* invalid. */
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#ab506a"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#e1d5bd"
      }
    },
    /* keyword. */
    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "#2c677e"
      }
    },
    /* markup. */
    /* meta. */
    {
      "scope": ["meta.tag", "meta.brace"],
      "settings": {
        "foreground": "#26233a"
      }
    },
    {
      "scope": ["meta.import", "meta.export"],
      "settings": {
        "foreground": "#2c677e"
      }
    },
    /* punctuation. */
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#65618e"
      }
    },
    {
      "scope": ["punctuation.accessor"],
      "settings": {
        "foreground": "#2c677e"
      }
    },
    {
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#f8ae48"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#7eb7c080"
      }
    },
    /* source. */
    /* storage. */
    {
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#2c677e"
      }
    },
    /* string. */
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#f8ae48"
      }
    },
    /* support. */
    {
      "scope": ["support"],
      "settings": {
        "foreground": "#7eb7c0"
      }
    },
    {
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#f8ae48"
      }
    },
    {
      "scope": ["support.function"],
      "settings": {
        "foreground": "#ab506a"
      }
    },
    /* text. */
    /* variable. */
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#cb9391",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable.other",
        "variable.language",
        "variable.function",
        "variable.argument"
      ],
      "settings": {
        "foreground": "#26233a"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#9a86b1"
      }
    }
  ]
}
