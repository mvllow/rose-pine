  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "_fg-inactive",
        "fontStyle": "italic"
      }
    },

    {
      "scope": ["constant"],
      "settings": {
        "foreground": "_pine"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.charcter.escape"
      ],
      "settings": {
        "foreground": "_rose"
      }
    },

    {
      "scope": ["entity.name"],
      "settings": {
        "foreground": "_rose"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag",
        "entity.name.namespace",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "_foam"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "entity.other.inherited-class"],
      "settings": {
        "foreground": "_gold",
        "fontStyle": "italic"
      }
    },

    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "_love"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "_fg-inactive"
      }
    },

    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "_pine"
      }
    },

    {
      "scope": ["meta.tag", "meta.brace"],
      "settings": {
        "foreground": "_fg"
      }
    },
    {
      "scope": ["meta.import", "meta.export"],
      "settings": {
        "foreground": "_pine"
      }
    },

    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "_fg-inactive"
      }
    },
    {
      "scope": ["punctuation.accessor"],
      "settings": {
        "foreground": "_pine"
      }
    },
    {
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "_leaf"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "_foam-muted"
      }
    },

    {
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "_pine"
      }
    },

    {
      "scope": ["string"],
      "settings": {
        "foreground": "_leaf"
      }
    },

    {
      "scope": ["support"],
      "settings": {
        "foreground": "_foam"
      }
    },
    {
      "scope": ["support.constant"],
      "settings": {
        "foreground": "_gold"
      }
    },
    {
      "scope": ["support.function"],
      "settings": {
        "foreground": "_love",
        "fontStyle": "italic"
      }
    },

    {
      "scope": ["variable"],
      "settings": {
        "foreground": "_rose",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable.other",
        "variable.language",
        "variable.function",
        "variable.argument"
      ],
      "settings": {
        "foreground": "_fg"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "_iris"
      }
    }
  ]
}
